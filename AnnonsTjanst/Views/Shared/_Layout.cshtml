<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Scripts.Render("~/bundles/modernizr")
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link href="~/assets/css/main.css" rel="stylesheet" />

</head>
<body>
    <header>
        <nav class="navbar navbar-expand-md navbar-dark">

            <a href='http://193.10.202.74/Anvandare/Profil'>
                <img class="navbar-brand mb-0 h1 nav-head" src="~/assets/img/handshake-logo-2.png" alt="Köp och sälj" />
            </a>

            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link" href='~/Redirect/sickar?url2=http%3A%2F%2F193.10.202.74/Anvandare/Profil/VisaProfil'><span class="d-md-none"> </span>Användare</a>@*href='http://193.10.202.74/Anvandare/Profil/VisaProfil?Id=@ViewBag.Id'*@
                    </li>
                    
                    <li class="nav-item">
                        <a class="nav-link" href='~/Redirect/sickar?url2=http%3A%2F%2F193.10.202.72/Client'><span class="d-md-none"> </span>Annonser</a>@*href='http://193.10.202.72/Client?Id=@ViewBag.Id*@
                    </li>

                    <li class="nav-item">
                        <a class="nav-link" href='~/Redirect/sickar?url2=http%3A%2F%2F193.10.202.73/betaltjanst'><span class="d-md-none"> </span>Betalningar</a>@*href='http://193.10.202.73/betaltjanst?Id=@ViewBag.Id'*@
                    </li>

                    <li class="nav-item">
                        <a class="nav-link" href='~/Redirect/sickar?url2=http%3A%2F%2F193.10.202.75/blogg'><span class="d-md-none"> </span>Bloggar</a>@*href='http://193.10.202.75/blogg?Id=@ViewBag.Id'*@
                    </li>
                </ul>
            </div>
        </nav>

    </header>

    <article @*class="container"*@ style="padding:10px">
        @RenderBody()
    </article>

    <footer>
        <hr>
        <div class="container">
            <p>© 2019 - Köp & Sälj</p>
        </div>
    </footer>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

</body>
</html>

